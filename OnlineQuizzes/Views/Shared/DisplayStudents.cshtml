@model OnlineQuizzes.Models.StudentInterest

<option value="">--Select Students--</option>

@if (ViewBag.InterestStudents != null)
{
    foreach (var item in ViewBag.InterestStudents)
    {
        <option value="@item.Value">@item.Text</option>
    }
}



@section scripts{
    <script type="text/javascript">
        $(".chzn-select").chosen();
    </script>
}